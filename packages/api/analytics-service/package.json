{
  "name": "@sparkapply/analytics-service",
  "version": "1.0.0",
  "description": "Advanced Analytics and Business Intelligence Service for SparkApply",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "build": "babel src -d dist",
    "docker:build": "docker build -t sparkapply/analytics-service .",
    "docker:run": "docker run -p 3003:3003 sparkapply/analytics-service",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "analytics:process": "node src/scripts/processAnalytics.js",
    "dashboard:generate": "node src/scripts/generateDashboards.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "sequelize": "^6.35.2",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "kafkajs": "^2.2.4",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "xlsx": "^0.18.5",
    "pdfkit": "^0.14.0",
    "sharp": "^0.33.1",
    "d3": "^7.8.5",
    "chart.js": "^4.4.0",
    "plotly.js": "^2.27.1",
    "mathjs": "^12.2.1",
    "ml-regression": "^6.0.1",
    "ml-matrix": "^6.10.7",
    "natural": "^6.8.0",
    "compromise": "^14.10.0",
    "sentiment": "^5.0.2",
    "node-nlp": "^4.27.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "brain.js": "^2.0.0-beta.23",
    "simple-statistics": "^7.8.3",
    "regression": "^2.0.1",
    "clustering-js": "^1.0.0",
    "kmeans-js": "^1.0.0",
    "elasticsearch": "^16.7.3",
    "prometheus-api-metrics": "^3.2.2",
    "prom-client": "^15.0.0",
    "jaeger-client": "^3.19.0",
    "opentracing": "^0.14.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-openapi-validator": "^5.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/cli": "^7.23.4",
    "sequelize-cli": "^6.6.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "analytics",
    "business-intelligence",
    "dashboard",
    "metrics",
    "reporting",
    "data-visualization",
    "machine-learning",
    "predictive-analytics",
    "real-time",
    "microservice"
  ],
  "author": "SparkApply Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frostlab63/sparkapply.git",
    "directory": "packages/api/analytics-service"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/config/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
